@model IEnumerable<Product>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Products</title>
</head>
<body>
    <h3>Ürünler</h3>
    <ul>
        @foreach (var item in Model)
        {
            <li>@item.Name - @item.Price</li>
        }
    </ul>

 

</body>
</html>
